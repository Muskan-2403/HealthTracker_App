<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings_nav"
    app:startDestination="@id/settingMain">

    <fragment
        android:id="@+id/settingMain"
        android:name="com.ultimate.infits.SettingMain"
        android:label="fragment_setting_main"
        tools:layout="@layout/fragment_setting_main" >
        <action
            android:id="@+id/action_settingMain_to_account2"
            app:destination="@id/account" />
        <action
            android:id="@+id/action_settingMain_to_achivement"
            app:destination="@id/achivement" />
    </fragment>
    <fragment
        android:id="@+id/account"
        android:name="com.ultimate.infits.Account"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/achivement"
        android:name="com.ultimate.infits.Achivement"
        android:label="fragment_achivement"
        tools:layout="@layout/fragment_achivement" />
</navigation>